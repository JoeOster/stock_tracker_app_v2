# This is a template for the .env file. Copy this to .env and fill in your values.

# Secret key for signing JWT tokens. Generate a strong, random string.
JWT_SECRET=your_super_secret_jwt_key_here

# Set to 'true' to enable authentication (login required).
# Set to 'false' to disable authentication and enable the user switcher for development.
ENABLE_AUTH=false

# Finnhub API Keys for price data. You can use one or two keys for load balancing.
FINNHUB_API_KEY=your_finnhub_api_key_1_here
FINNHUB_API_KEY_2=your_finnhub_api_key_2_here

# Total API calls allowed per minute across all keys.
# Finnhub free tier is 30 calls/minute per key. With two keys, you can set this to 60.
# If you have a paid plan, adjust accordingly.
API_CALLS_PER_MINUTE=60